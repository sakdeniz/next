  "private": false,
  "scripts": {
    "dev": "node build/dev-server.js",
    "dev:server": "node server.js",
    "start": "echo please use 'npm run dev' AND 'npm run dev:server'",
    "build": "cross-env ENABLE_ESLINT=false node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "rb": "npm rebuild zeromq --runtime=electron --target=2.0.7",
    "navcoind-start-testnet": "d:/next/bin/navcoind -testnet -rpcport=44445 -rpcuser=test -rpcpassword=test",
    "navcoind-stop-testnet": "d:/next/bin/navcoin-cli -testnet -rpcuser=test -rpcpassword=test -rpcport=44445 stop",
    "navcoind-start-devnet": "d:/next/bin/navcoind -devnet -rpcport=44446 -rpcuser=test -rpcpassword=test",
    "navcoind-stop-devnet": "d:/next/bin/navcoin-cli -devnet -rpcuser=test -rpcpassword=test -rpcport=44446 stop",
    "navcoind-start-mainnet": "d:/next/bin/navcoind -rpcport=44444 -rpcuser=test -rpcpassword=test",
    "navcoind-stop-mainnet": "d:/next/bin/navcoin-cli -rpcuser=test -rpcpassword=test -rpcport=44444 stop",
    "server-start": "node server.js",
    "package-windows-ia32": "electron-packager . --electron-version 1.8.2 --overwrite --executable-name next --platform win32 --arch ia32 --appname next --icon src/assets/next.ico",
    "package-windows-x64": "electron-packager . --electron-version 1.8.2 --overwrite --executable-name next --platform win32 --arch x64 --appname next --icon src/assets/next.ico",
    "package-linux-ia32": "electron-packager . --electron-version 1.8.2 --overwrite --executable-name next --platform linux --arch ia32 --appname next",
    "package-linux-x64": "electron-packager . --electron-version 1.8.2 --overwrite --executable-name next --platform linux --arch x64 --appname next",
    "package-linux-aarch64": "electron-packager . --electron-version 1.8.2 --overwrite --executable-name next --platform linux --arch arm64 appname next",
    "package-linux-arm64": "electron-packager . --electron-version 1.8.2 --overwrite --executable-name next --platform linux --arch arm64 appname next",
    "package-linux-armv7l": "electron-packager . --electron-version 1.8.2 --overwrite --executable-name next --platform linux --arch armv7l appname next",
    "package-macos-x64": "electron-packager . --electron-version 1.8.2 --overwrite --executable-name next --platform darwin --arch x64 --appname next",
    "sign-macos-x64": "codesign --deep --verbose --force --sign 'Developer ID Application: Seyhan Akdeniz (882SDNXSH8)' next-darwin-x64/next.app",
    "eb": "electron-builder --ia32 --x64 electron-builder.yml"
  },
